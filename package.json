{
  "name": "english-punch-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "convex dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint src convex",
    "lint:fix": "eslint src convex --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css}\" \"convex/**/*.{ts,tsx}\"",
    "knip": "knip",
    "change-password": "tsx src/scripts/changePassword.ts",
    "vibe-rules": "npx vibe-rules install codex && prettier --write AGENTS.md"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.90",
    "@fabianlars/tauri-plugin-oauth": "^2.0.0",
    "@google/genai": "^1.30.0",
    "@tanstack/react-router": "^1.139.10",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-adapter": "^1.139.10",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.4.5",
    "@tauri-apps/plugin-opener": "^2",
    "clsx": "^2.1.1",
    "convex": "^1.31.2",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "ts-fsrs": "^5.2.3",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.1.1",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/router-plugin": "^1.139.10",
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.14",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "knip": "^5.70.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
